CMD:dealershiphelp(playerid, params[])
{
	SCM(playerid, SERVER_COLOR, "Dealership Commands: /createcdveh, /deletecdveh, /editcarspawn, /editcarprice, /editcarprice, /dealershiprespawn, /dl (Pc Only).");
}

CMD:dealershiprespawn(playerid, params[])
{
	new businessid = GetInsideBusiness(playerid);
	if(businessid == -1 || !IsBusinessOwner(playerid, businessid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "You're not near any business that you own.");
	}
	if(businessid == INVALID_BUSINESS_ID) return SM(playerid, COLOR_GRAD2, "You are not in a business, use /switchbiz first.");
	SM(playerid, COLOR_GREY, "** Respawning dealership vehicles..");
	for (new i; i < MAX_BUSINESS_DEALERSHIP_VEHICLES; i++)
	{
		if(BusinessInfo[businessid][bVehID][i] != 0) SetVehicleToRespawn(BusinessInfo[businessid][bVehID][i]);
	}
	return 1;
}

CMD:editcarprice(playerid, params[])
{
	new businessid = GetNearbyCarBusinessEx(playerid);
    //new businessid = GetInsideBusiness(playerid);
	if(businessid == -1 || !IsBusinessOwner(playerid, businessid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "You're not near any business that you own.");
	}
	else if(BusinessInfo[businessid][bType] != BUSINESS_DEALERSHIP) {
		SM(playerid, COLOR_GREY, "You don't own a vehicle dealership.");
	}
	else if(!IsPlayerInRangeOfPoint(playerid, 20.0, BusinessInfo[businessid][bPosX], BusinessInfo[businessid][bPosY], BusinessInfo[businessid][bPosZ])) {
		SM(playerid, COLOR_GREY, "You need to be standing near the dealership entrance.");
	}
	else {
		new vehicleid, amount;
		if(sscanf(params, "dd", vehicleid, amount)) {
			SM(playerid, COLOR_GREY, "USAGE: /editcarprice [VehicleID] [Price]");
		}
		else if(businessid != GetCarBusiness(vehicleid)) {
			SM(playerid, COLOR_GREY, "That vehicle is not part of your dealership.");
		}
		else if(amount < 0) {
			SM(playerid, COLOR_GREY, "The price can't be set below 0");
		}
		else {

 			new
				iSlot = GetBusinessCarSlot(vehicleid),
				Message[128];

			BusinessInfo[businessid][bVPrice][iSlot] = amount;
			format(Message, sizeof(Message), "%s For Sale | Price: $%s", GetVehicleName(BusinessInfo[businessid][bVehID][iSlot]), number_format(BusinessInfo[businessid][bVPrice][iSlot]));
            UpdateDynamic3DTextLabelText(BusinessInfo[businessid][bVehicleLabel][iSlot], SERVER_COLOR, Message);
			format(Message, sizeof(Message), "%s price has been set to $%s", GetVehicleName(vehicleid), number_format(amount));
			SM(playerid, COLOR_WHITE, Message);
			SaveDealershipVehicle(businessid, iSlot);
		}
	}
	return 1;
}

CMD:deletecdveh(playerid, params[]) {
	if(PlayerInfo[playerid][pAdmin] >= 4) {

		new
		    iBusiness,
			iVehicle;

		if(sscanf(params, "ii", iBusiness, iVehicle )) {
			SM(playerid, COLOR_GREY, "USAGE: /deletecdveh [business ID] [model id]");
		}
		else if(BusinessInfo[iBusiness][bType] != BUSINESS_DEALERSHIP) {
			SM(playerid, COLOR_GRAD2, "Business is not a car dealership!");
		}
		else if(iBusiness != GetCarBusiness(iVehicle)) {
			SM(playerid, COLOR_GREY, "That vehicle isn't a dealership vehicle.");
		}
		else {

			new
				ID = GetBusinessCarSlot(iVehicle);

			if(BusinessInfo[iBusiness][bVehID][ID] != INVALID_VEHICLE_ID) {
			    if(IsValidDynamic3DTextLabel(BusinessInfo[iBusiness][bVehicleLabel][ID])) DestroyDynamic3DTextLabel(BusinessInfo[iBusiness][bVehicleLabel][ID]), BusinessInfo[iBusiness][bVehicleLabel][ID] = Text3D:-1;
                DestroyVehicle(BusinessInfo[iBusiness][bVehID][ID]);
               	BusinessInfo[iBusiness][bModel][ID] = 0;
				BusinessInfo[iBusiness][bParkPosX][ID] = 0;
  				BusinessInfo[iBusiness][bParkPosY][ID] = 0;
			   	BusinessInfo[iBusiness][bParkPosZ][ID] = 0;
		   	 	BusinessInfo[iBusiness][bParkAngle][ID] = 0;
		   	 	BusinessInfo[iBusiness][bVehID][ID] = 0;
		   	 	BusinessInfo[iBusiness][bVPrice][ID] = 0;
		   	 	SaveDealershipVehicle(iBusiness, ID);
	   	 		return 1;
       		}
			return SM(playerid, COLOR_GREY, "The max number of vehicles for this business has been reached.");
		}
	}
	else SM(playerid, COLOR_GRAD1, "You are not authorized to use that command.");
	return 1;
}


CMD:createcdveh(playerid, params[])
{
	new iBusiness, iVehicle, iColors[2], Float: fVehPos[4], label[50], model[20];
	if(sscanf(params, "is[20]ii", iBusiness, model, iColors[0], iColors[1]))
	{
		return SCM(playerid, COLOR_GREY, "USAGE: /createcdveh [business ID] [model id] [color 1] [color 2]");
	}
	if(PlayerInfo[playerid][pAdmin] < 4)
	{
		return SCM(playerid, COLOR_GRAD1, "You are not authorized to use that command.");
	}
	if((iVehicle = GetVehicleModelByName(model)) == 0)
	{
	    return SCM(playerid, COLOR_SYNTAX, "Invalid vehicle model.");
	}
	if(!(-1 <= iColors[0] <= 255) || !(-1 <= iColors[1] <= 255))
	{
	    return SCM(playerid, COLOR_SYNTAX, "Invalid color. Valid colors range from -1 to 255.");
	}
	if(BusinessInfo[iBusiness][bType] != BUSINESS_DEALERSHIP)
	{
		return SCM(playerid, COLOR_GRAD2, "Business is not a car dealership!");
	}
	for(new i; i < MAX_BUSINESS_DEALERSHIP_VEHICLES; i++)
	{
		if(BusinessInfo[iBusiness][bVehID][i] == 0)
		{
			GetPlayerPos(playerid, fVehPos[0], fVehPos[1], fVehPos[2]);
			GetPlayerFacingAngle(playerid, fVehPos[3]);
			BusinessInfo[iBusiness][bVehID][i] = CreateVehicle(iVehicle, fVehPos[0], fVehPos[1], fVehPos[2], fVehPos[3], iColors[0], iColors[1], -1);

			BusinessInfo[iBusiness][bModel][i] = iVehicle;

			BusinessInfo[iBusiness][bParkPosX][i] = fVehPos[0];
			BusinessInfo[iBusiness][bParkPosY][i] = fVehPos[1];
			BusinessInfo[iBusiness][bParkPosZ][i] = fVehPos[2];
			BusinessInfo[iBusiness][bParkAngle][i] = fVehPos[3];

			format(label, sizeof(label), "%s For Sale | Price: $%s", GetVehicleName(BusinessInfo[iBusiness][bVehID][i]), number_format(BusinessInfo[iBusiness][bVPrice][i]));
			BusinessInfo[iBusiness][bVehicleLabel][i] = CreateDynamic3DTextLabel(label,SERVER_COLOR,BusinessInfo[iBusiness][bParkPosX][i], BusinessInfo[iBusiness][bParkPosY][i], BusinessInfo[iBusiness][bParkPosZ][i],8.0,INVALID_PLAYER_ID, BusinessInfo[iBusiness][bVehID][i]);

			BusinessInfo[iBusiness][DealershipVehStock][i] = 1;
			SaveDealershipVehicle(iBusiness, i);
			return 1;
		}
	}
	return 1;
}

CMD:editcarspawn(playerid, params[])
{
    new businessid = GetNearbyCarBusinessEx(playerid);
    if(businessid == -1 || !IsBusinessOwner(playerid, businessid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "You're not near any business that you own.");
	}
	else if(BusinessInfo[businessid][bType] != BUSINESS_DEALERSHIP) {
		SM(playerid, COLOR_GREY, "You don't own a vehicle dealership.");
	}
	else if(!IsPlayerInRangeOfPoint(playerid, 20.0, BusinessInfo[businessid][bPosX], BusinessInfo[businessid][bPosY], BusinessInfo[businessid][bPosZ])) {
		SM(playerid, COLOR_GREY, "The location needs to be near the business entrance.");
	}
	else {
		new Float: Positionsz[4];
		GetPlayerPos(playerid, Positionsz[0], Positionsz[1], Positionsz[2]);
		GetPlayerFacingAngle(playerid, Positionsz[3]);
