		}
		else
		{
			color = FactionInfo[faction][fColor];
		} 
		format(string, sizeof(string), "{%06x}%s\n"WHITE"%s", color >>> 8, FactionInfo[faction][fName],FactionRanks[faction][PlayerInfo[playerid][pFactionRank]]);
