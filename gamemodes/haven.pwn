		//Log_Write("logs/hack.log", string);
        PlayerInfo[playerid][pAdvertWarnings] ++;
		return 0;
	}

	PlayerInfo[playerid][pAFKPos][0] = 0.0;
    PlayerInfo[playerid][pAFKPos][1] = 0.0;
    PlayerInfo[playerid][pAFKPos][2] = 0.0;
	return 1;
}

