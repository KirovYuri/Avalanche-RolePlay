	foreach(new i : Player)
	{
		if((PlayerInfo[i][pHelper] > 0 || PlayerInfo[playerid][pAdmin] > 0) && !PlayerInfo[i][pToggleHelper])
	    {
	        if(strlen(params) > MAX_SPLIT_LENGTH)
	        {
	            SendStaffMessage(0x42f4EEFF, "** [%s] %s %s: %.*s... **", IsPlayerAndroid(playerid) ? ("CP") : ("PC"), GetStaffRank(playerid), GetRPName(playerid), MAX_SPLIT_LENGTH, params);
