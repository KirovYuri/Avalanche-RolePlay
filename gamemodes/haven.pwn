LevelCheck(playerid)
{
	if (PlayerInfo[playerid][pLogged] != 0)
	{
		if(PlayerInfo[playerid][pLevel] >= 0)
		{
			new nxtlevel = PlayerInfo[playerid][pLevel]+1;
			new expamount = nxtlevel*4;

			if (PlayerInfo[playerid][pEXP] < expamount)
			{
				return 0;
			}
			else if(PlayerInfo[playerid][pEXP] > expamount)
			{
				while(PlayerInfo[playerid][pEXP] > expamount) 
				{
					PlayerInfo[playerid][pLevel]++;
					PlayerInfo[playerid][pEXP] = PlayerInfo[playerid][pEXP]-expamount;
					SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);
					nxtlevel = PlayerInfo[playerid][pLevel]+1;
					expamount = nxtlevel*4;

					mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET level = level + 1, exp = exp - %i  WHERE uid = %i", PlayerInfo[playerid][pLevel], PlayerInfo[playerid][pEXP], PlayerInfo[playerid][pID]);
					mysql_tquery(connectionID, queryBuffer);
				}
				SendClientMessageEx(playerid, COLOR_WHITE, "You had an excess amount of respect points, hence your level was adjusted to %d.", PlayerInfo[playerid][pLevel]);
				return 1;
			}
			else
			{
				new string[125];
				format(string, sizeof(string), "~g~LEVEL UP~n~~w~You Are Now Level %d", nxtlevel);
				PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
				PlayerInfo[playerid][pLevel]++;
				PlayerInfo[playerid][pEXP] = PlayerInfo[playerid][pEXP]-expamount;
				GameTextForPlayer(playerid, string, 5000, 1);
				format(string, sizeof(string), "{FFFFFF}You have leveled up to %d!!!.", nxtlevel);
				SetPlayerScore(playerid, PlayerInfo[playerid][pLevel]);

				mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET level = level + 1, exp = exp - %i  WHERE uid = %i", PlayerInfo[playerid][pLevel], PlayerInfo[playerid][pEXP], PlayerInfo[playerid][pID]);
				mysql_tquery(connectionID, queryBuffer);
	            if(PlayerInfo[playerid][pLevel] == 3 && PlayerInfo[playerid][pReferralUID] > 0)
	            {
	                ReferralCheck(playerid);
	            }
				if(PlayerInfo[playerid][pLevel] == 6)
				{
				    SCM(playerid, COLOR_WHITE, "{FFFFFF}Newbie chat will now be automatically togged off on login.");
				}
			}
		}
		return 1;
	}
	else
	{
		SendClientMessageEx(playerid, COLOR_GRAD1, "You're not logged in.");
	}
    SaveServerInfo();
	return 1;
}

stock UpdateVehicleHUDForPlayer(p, fuel, speed)
{
	new str[128], vehicleid = GetPlayerVehicleID(p), szColor[4];
	new engine,lights,alarm,doors,bonnet,boot,objective;
	GetVehicleParamsEx(vehicleid,engine,lights,alarm,doors,bonnet,boot,objective);
	switch(speed)
	{
	    case 0..40: szColor = "~w~";
	    case 41..60: szColor = "~y~";
	    default: szColor = "~r~";
	}

	if (IsVIPcar(vehicleid) || GetVehicleModel(vehicleid) == 481 || GetVehicleModel(vehicleid) == 509 || GetVehicleModel(vehicleid) == 510)
		format(str, sizeof(str), "~b~Fuel: ~w~U", fuel);
	else{
		format(str, sizeof(str), "~b~Fuel: ~w~%i",fuel);
		PlayerTextDrawSetString(p, _vhudTextFuel[p], str);

		format(str, sizeof(str), "~b~MPH: %s%i",szColor, speed);
		PlayerTextDrawSetString(p, _vhudTextSpeed[p], str);
	}
	if(ExBJck[p] == 0)
	{
		format(str, sizeof(str), "~b~%s: ~r~OFF", IsAMotorBike(vehicleid) ? ("HM"):("SB"));
		PlayerTextDrawSetString(p, _vhudSeatBelt[p], str);
	}
	else if(ExBJck[p] == 1) {
		format(str, sizeof(str), "~b~HM: ~g~ON");
		PlayerTextDrawSetString(p, _vhudSeatBelt[p], str);
	}
	else {
		format(str, sizeof(str), "~b~SB: ~g~ON");
		PlayerTextDrawSetString(p, _vhudSeatBelt[p], str);
	}
	if(lights != VEHICLE_PARAMS_ON) {
		format(str, sizeof(str), "~b~Lights: ~r~OFF");
		PlayerTextDrawSetString(p, _vhudLights[p], str);	
	}
	else {
		format(str, sizeof(str), "~b~Lights: ~g~ON");
		PlayerTextDrawSetString(p, _vhudLights[p], str);
	}
}

