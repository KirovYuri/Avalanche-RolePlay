 	format(super, sizeof(super), "{FFFFFF}(Age: %i)\n", PlayerInfo[playerid][pAge]);
 	strcat(str, super);
 	format(super, sizeof(super), "{FFFFFF}(Married to: %s)\n", PlayerInfo[playerid][pMarriedName]);
 	strcat(str, super);
	format(super, sizeof(super), "{FFFFFF}(Insurance: %s)\n", insurance);
