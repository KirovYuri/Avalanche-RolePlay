 	strcat(str, super);
 	format(super, sizeof(super), "{FFFFFF}(Phone Number: %i)\n", PlayerInfo[playerid][pPhone]);
 	strcat(str, super);
 	format(super, sizeof(super), "{FFFFFF}(Channel: %i)\n", PlayerInfo[playerid][pChannel]);
 	strcat(str, super);
