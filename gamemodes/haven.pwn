public OnIncomingConnection(playerid, ip_address[], port)
{
    new
        count = 1;
 
    for (new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
    {
        if (IsPlayerConnected(i) && strcmp(GetPlayerIP(i), ip_address, false) == 0) 
        {
            count ++;
        }
    }
 
    if (count > MAX_CONNECTIONS)
    {
		SMA(COLOR_LIGHTRED, "AdmCmd: %s was autokicked by %s, reason: Many Connections", GetRPName(playerid), SERVER_BOT);
        KickPlayer(playerid);
    }
    return true;
}

public OnPlayerDisconnect(playerid, reason)
{
    // Discord Bot Status
    online--;
	new string[150];
    format(string, sizeof(string), "[Disconnect]: {FFFFFF}%s has logged out {1CDBE7}[IP: %s] [Platform: %s]", GetRPName(playerid), GetPlayerIP(playerid), IsPlayerAndroid(playerid) ? ("CP") : ("PC"));
	SendAdminMessage(SERVER_COLOR, string, 2);
 	KillTimer(HJLimitTimer);
	HackerJob[playerid] = 0;
	PlayerInfo[playerid][pConstructionWork] = 0;
    // Kill the timer and reset the value to invalid
    KillTimer(gPlayerPingTimer[playerid]);
    gPlayerPingTimer[playerid] = -1;
	if(CarryCrate[playerid] != -1)
	{
		PlayerCarryCrate(playerid, CarryCrate[playerid], -1, 1);
	}
	if(PlayerInfo[playerid][pLockBreak] != INVALID_VEHICLE_ID)
	{
	    CancelBreakIn(playerid);
	}	
	// Sabong System
	if(Boxing[playerid])
	{
        // Getting other boxer's ID
		new playerb = -1;
		foreach(new i : Player)
        {
            if(Boxing[i] && i != playerid) playerb = i;
        }
        if(playerb != -1)
        {
            format(string, sizeof(string), "[Sabong System]: "WHITE"The sabong match has ended, %s has won the match.", GetRPName(playerb));
    		SMA(COLOR_GREY2, string);
			// PlayerLastSkin
			SetPlayerSkin(playerid, BoxingSkin[playerid]);
			SetPlayerSkin(playerb, BoxingSkin[playerb]);
            // Putting them out of the ring
            SetPlayerPos(playerb, 386.1609,1278.3429,-56.8100);
            SetPlayerFacingAngle(playerb, 2.6789);
            SetPlayerPos(playerid, 386.1609,1278.3429,-56.8100);
            SetPlayerFacingAngle(playerid, 2.6789);
			// Variables
			SavePlayerVariables(playerid);
			SavePlayerVariables(playerb);
			// PlayerHealth
			SetPlayerHealth(playerid, 100.0);
			SetPlayerHealth(playerb, 100.0);
			SendClientMessage(playerb, COLOR_GREY2, "[Sabong System]: "YELLOW"You have won the sabong match.");
			SendClientMessage(playerid, COLOR_GREY2, "[Sabong System]: "RED"You have lost the sabong match.");
			// Giving Bet
			new winner = pBoxer[playerb];
			foreach(new i : Player)
			{
			    if(BidBoxer[i] == winner)
			    {
			        new percent, profit;
			        percent = (BidAmount[i]/BoxerBid[winner]) * 100;
			        profit = (percent * BoxerBid[pBoxer[playerid]]) / 100;
			        GivePlayerCash(i, BidAmount[i] + profit);
			        format(string, sizeof(string), "[Sabong System]: "WHITE"You have won $%i from betting in the match, the money you bet has been returned as well.", profit);
			        SendClientMessage(i, COLOR_GREY2, string);
			    }
				BidAmount[i] = 0;
				BidBoxer[i] = 0;
			}
        }
        // Resetting Values
		BoxingMatch = 0;
		Boxer[1] = -1;
		pBoxer[playerb] = 0;
		Boxer[2] = -1;
		pBoxer[playerid] = 0;
		Boxing[playerb] = 0;
		Boxing[playerid] = 0;
		BoxerBid[1] = 0;
		BoxerBid[2] = 0;
	    if(BoxTime)
	    {
			TogglePlayerControllable(playerb, 1);
	    	KillTimer(BoxTimer);
	    }
    }
	if(PoolAimer == playerid)
	{
		PoolAimer = -1;
		TextDrawHideForPlayer(playerid, PoolTD[0]);
		TextDrawHideForPlayer(playerid, PoolTD[1]);
		TextDrawHideForPlayer(playerid, PoolTD[2]);
		TextDrawHideForPlayer(playerid, PoolTD[3]);
		DestroyObject(AimObject);
	}
	if(PlayingPool[playerid])
	{
		PlayingPool[playerid] = 0;
		new
			count = GetPoolPlayersCount();
		if(count <= 0)
		{
			PoolStarted = 0;
			RespawnPoolBalls();
		}
	}
	if(lstream[playerid])
    {
        SvDeleteStream(lstream[playerid]);
        lstream[playerid] = SV_NULL;
    }
	TerminateInfo(playerid, reason);
	Bit_Off(arrPlayerBits[playerid], taketest_bitState);
	return 1;
}

public OnPlayerRequestClass(playerid, classid)
{
    if(PlayerInfo[playerid][pKicked]) return 0;
    if(IsPlayerNPC(playerid)) return 1;
    
    SetTimerEx("ShowMainMenuCamera", 400, false, "i", playerid);
	if(!PlayerInfo[playerid][pLogged])
	{
	    ClearChat(playerid);
    	for(new i = 0; i < 5; i ++)
		{
		    SCM(playerid, -1, " ");
		}

		TogglePlayerSpectating(playerid, 1);
        SetPlayerColor(playerid, 0xFFFFFF00);
		// Due to a SA-MP bug, you can't apply camera coordinates directly after enabling spectator mode (to hide HUD).
		// In this case we'll use a timer to defer this action.
        InterpolateCameraPos(playerid, 2209.850585, -1115.027709, 37.585510, 2019.370605, -1123.279907, 45.939300, 50000);
        InterpolateCameraLookAt(playerid, 2205.017089, -1116.304321, 37.493427, 2017.365600, -1127.638549, 44.531455, 50000);
	}

	return 1;
}

