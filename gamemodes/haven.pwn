{
    new houseid = GetInsideHouse(playerid), landid = GetNearbyLand(playerid),id;

	if((id = GetNearbyLand(playerid)) >= 0 && (IsLandOwner(playerid, id) || PlayerInfo[playerid][pLandPerms] == id))
