CMD:graduate(playerid, params[])
{
	new targetid, status, grad[32];
	if(!PlayerInfo[playerid][pTeacher])
	{
		return SCM(playerid, COLOR_SYNTAX, "You cannot use this command as you're not teacher.");
	}
	if(sscanf(params, "ui", targetid, status) || !(0 <= status <= 6))
	{
	    SCM(playerid, COLOR_SYNTAX, "Usage: /graduate [playerid] [status (0/1/2/3/4/5/6)]");
		SCM(playerid, COLOR_WHITE, "Options: 0 = None, 1 = Criminology, 2 = Doctoral/Fire, 3 = TESDA, 4 = Journalism, 5 = Law, 6 = HighSchool");
		return 1;
	}
	if(!IsPlayerConnected(targetid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "The player specified is disconnected.");
	}
	if(!PlayerInfo[targetid][pLogged])
	{
	    return SCM(playerid, COLOR_SYNTAX, "That player hasn't logged in yet.");
	}
	if(!IsPlayerConnected(targetid) || !IsPlayerInRangeOfPlayer(playerid, targetid, 5.0))
	{
	    return SCM(playerid, COLOR_SYNTAX, "The player specified is disconnected or out of range.");
	}

	if(status == 1) grad = "Criminology";
	else if(status == 2) grad = "Doctoral";
	else if(status == 3) grad = "TESDA";
	else if(status == 4) grad = "Journalism";
	else if(status == 5) grad = "Law";
	else if(status == 6) grad = "High School";

	PlayerInfo[targetid][pCourse] = status;
	if(status > 0)
	{
		SMA(COLOR_GREEN, "%s has graduated %s in Star University.", GetRPName(targetid), grad);
	}
	else if(status == 0)
	{
		SMA(COLOR_RED, "%s revoked %s's diploma in Star University.", GetRPName(playerid), GetRPName(targetid));
	}
	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET course = %i WHERE uid = %i", PlayerInfo[targetid][pCourse], PlayerInfo[targetid][pID]);
	mysql_tquery(connectionID, queryBuffer);

	return 1;
}

