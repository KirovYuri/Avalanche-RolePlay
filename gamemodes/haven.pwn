	strcat(str, super);
	format(super, sizeof(super), "{FFFFFF}(VIP Package: %s)\n\n", GetDonatorRank(PlayerInfo[playerid][pVIPPackage]));
	strcat(str, super);
 	format(super, sizeof(super), "{FFFFFF}(Name: %s)\n", name);
