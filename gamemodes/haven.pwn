CMD:bet(playerid, params[])
{
	new amount1, amount2, option[10], param[32], string[128];
	if(!PlayerInfo[playerid][pLogged])
	{
	    return SCM(playerid, COLOR_SYNTAX, "That player hasn't logged in yet.");
	}
	if(!enablesabong && PlayerInfo[playerid][pAdmin] < 5)
	{
	    return SCM(playerid, COLOR_SYNTAX, "The Sabong System is disabled at the moment.");
	}
	if(!BoxingMatch)
	{
		return SCM(playerid, COLOR_GREY, "There is no sabong match going on.");
	}
	if(!BoxTime)
	{
		return SCM(playerid, COLOR_GREY, "The sabong match has already started.");
	}
	if(BidAmount[playerid])
	{
		return SCM(playerid, COLOR_GREY, "You have already placed your bet for this match.");
	}
	if(sscanf(params, "s[10]S()[32]", option, param))
	{
		return SCM(playerid, COLOR_SYNTAX, "Usage: /bet [meron/wala] [amount]");
	}
 	if(!strcmp(option, "meron", true))
	{
	    if(sscanf(param, "i", amount1))
	    {
	        return SCM(playerid, COLOR_SYNTAX, "Usage: /bet [meron/wala] [amount]");
		}
		if(PlayerInfo[playerid][pCash] < amount1)
	    {
	        return SCM(playerid, COLOR_SYNTAX, "You don't have that much money to bet.");
	    }
	    if(amount1 < 100)
	    {
	    	return SCM(playerid, COLOR_GREY, "Minimum bet is $100.");
	    }
		BetMeron += amount1;
		BetVoted[playerid] = 1;
	    BoxerBid[2] += amount1;
	    BidAmount[playerid] = amount1;
	    BidBoxer[playerid] = Boxer[2];
	    GivePlayerCash(playerid, -amount1);
	    for(new i = 0; i < 14; i ++)
	    {
            TextDrawShowForPlayer(playerid, SabongTD[i]);
	        format(string, sizeof(string), "%s", GetRPName(Boxer[1]));
            TextDrawSetString(SabongTD[7], string);
	        format(string, sizeof(string), "%s", GetRPName(Boxer[2]));
            TextDrawSetString(SabongTD[8], string);
	        format(string, sizeof(string), "$%s", AddCommas(BetMeron));
            TextDrawSetString(SabongTD[9], string);
	        format(string, sizeof(string), "$%s", AddCommas(BetWala));
            TextDrawSetString(SabongTD[10], string);
	        format(string, sizeof(string), "Payout = ~g~$%s", AddCommas(amount1));
            TextDrawSetString(SabongTD[12], string);
			SetTimerEx("SABONGHIDE", 60000, false, "i", i);
	    }
	}
	else if(!strcmp(option, "wala", true))
	{
	    if(sscanf(param, "i", amount2))
	    {
	        return SCM(playerid, COLOR_SYNTAX, "Usage: /bet [meron/wala] [amount]");
		}
		if(PlayerInfo[playerid][pCash] < amount2)
	    {
	        return SCM(playerid, COLOR_SYNTAX, "You don't have that much money to bet.");
	    }
	    if(amount2 < 100)
	    {
	    	return SCM(playerid, COLOR_GREY, "Minimum bet is $100.");
	    }
		BetWala += amount2;
		BetVoted[playerid] = 1;
	    BoxerBid[1] += amount2;
	    BidAmount[playerid] = amount2;
	    BidBoxer[playerid] = Boxer[1];
	    GivePlayerCash(playerid, -amount2);
	    for(new i = 0; i < 14; i ++)
	    {
            TextDrawShowForPlayer(playerid, SabongTD[i]);
	        format(string, sizeof(string), "%s", GetRPName(Boxer[1]));
            TextDrawSetString(SabongTD[7], string);
	        format(string, sizeof(string), "%s", GetRPName(Boxer[2]));
            TextDrawSetString(SabongTD[8], string);
	        format(string, sizeof(string), "$%s", AddCommas(BetMeron));
            TextDrawSetString(SabongTD[9], string);
	        format(string, sizeof(string), "$%s", AddCommas(BetWala));
            TextDrawSetString(SabongTD[10], string);
	        format(string, sizeof(string), "Payout = ~g~$%s", AddCommas(amount2));
            TextDrawSetString(SabongTD[13], string);
			SetTimerEx("SABONGHIDE", 60000, false, "i", i);
	    }
	}
	return 1;
}

