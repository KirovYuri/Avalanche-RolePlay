CMD:gather(playerid, params[])
{
    new vehicleid = GetPlayerVehicleID(playerid);
    
    if(!PlayerHasJob(playerid, JOB_FARMER))
	{
	    return SendClientMessage(playerid, COLOR_GREY, "You can't use this command as you are not a Farmer.");
	}
	
    if(farmVehicles[0] <= vehicleid <= farmVehicles[6])
    {
		PlayerInfo[playerid][pCP] = CHECKPOINT_FARM;
		PlayerInfo[playerid][pFarmVeh] = vehicleid;
		PlayerInfo[playerid][pFarmCP] = 0;

        GameTextForPlayer(playerid, "~y~Procced to the checkpoint", 2050, 3);

	    SetPlayerCheckpoint(playerid, farmVehCPs[PlayerInfo[playerid][pFarmCP]][0], farmVehCPs[PlayerInfo[playerid][pFarmCP]][1], farmVehCPs[PlayerInfo[playerid][pFarmCP]][2], 3.0);
	    SCM(playerid, COLOR_AQUA, "Drive through the checkpoints to proceed.");
    }
    else
    {
        SCM(playerid, COLOR_GREY, "You must be in a walton vehicle as a driver");
    }
	return 1;
}

CMD:startsweeping(playerid, params[])
{
	if(!PlayerHasJob(playerid, JOB_SWEEPER))
	{
	    return SendClientMessage(playerid, COLOR_GREY, "You can't use this command as you're not a Street Sweeper.");
	}
	if(PlayerInfo[playerid][pSweeping])
	{
	    return SendClientMessage(playerid, COLOR_GREY, "You are sweeping already. /stopsweeping to stop.");
	}
	if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 574)
	{
	    return SendClientMessage(playerid, COLOR_GREY, "You are not sitting inside a Sweeper.");
	}

	PlayerInfo[playerid][pSweeping] = 1;
	PlayerInfo[playerid][pSweepTime] = 30;
	PlayerInfo[playerid][pSweepEarnings] = 500;

	SendClientMessage(playerid, COLOR_WHITE, "** You are now sweeping. Drive around with your sweeper to earn money towards your paycheck.");
	SendClientMessage(playerid, COLOR_WHITE, "** When you are finished your sweeping shift, use /stopsweeping in order to end your shift.");
	return 1;
}

CMD:stopsweeping(playerid, params[])
{
	if(!PlayerHasJob(playerid, JOB_SWEEPER))
	{
	    return SendClientMessage(playerid, COLOR_GREY, "You can't use this command as you're not a Street Sweeper.");
	}
	if(!PlayerInfo[playerid][pSweeping])
	{
	    return SendClientMessage(playerid, COLOR_GREY, "You are not sweeping right now.");
	}

	SM(playerid, COLOR_SYNTAX, "** You are no longer sweeping. You earned a total of {00AA00}$%i{33CCFF} towards your paycheck during your shift.", PlayerInfo[playerid][pSweepEarnings]);
    PlayerInfo[playerid][pSweeping] = 0;
	PlayerInfo[playerid][pSweepTime] = 0;
	PlayerInfo[playerid][pSweepEarnings] = 500;
	return 1;
}

