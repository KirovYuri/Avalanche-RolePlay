DrivingTest(playerid)
{
	if(!Bit_State(arrPlayerBits[playerid], taketest_bitState))
	{
		for(new i = 0; i < 8; i++)
		{
			SelectTextDraw(playerid, 0x33AA33AA);
			PlayerTextDrawShow(playerid, DrivingQuestion1[playerid][i]);
		}
		Bit_On(arrPlayerBits[playerid], taketest_bitState);
		SetPVarInt(playerid, "takingtest", 1);
		UsingDriversTest[playerid] = 1;
	}
	else
	{
		HideDrivTest(playerid);
		Bit_Off(arrPlayerBits[playerid], taketest_bitState);
	}
	return 1;
}

