		GivePlayerHealth(playerid, 10.0);

		PlayerInfo[playerid][pPot] -= 1;
		PlayerInfo[playerid][pDrugsUsed]++;
		PlayerInfo[playerid][pLastDrug] = gettime();

		if(PlayerInfo[playerid][pDrugsUsed] >= 4)
