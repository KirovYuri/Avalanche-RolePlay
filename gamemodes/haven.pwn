CMD:getpackage(playerid, params[])
{
	new slots = GetCrateWeaponCapacity(CAPACITY_CRATEWEP);
	new value;
	new boxid = GetCrateBox(playerid);
	if(sscanf(params, "i", value))
	{
		return SM(playerid, COLOR_SYNTAX, "Usage: /getpackage [slot]", slots);
	}
	if(boxid == -1)
	{
		return SCM(playerid, COLOR_SYNTAX, "You're not near to any crate box!");
	}
	if(!(1 <= value <= slots))
	{
		return SCM(playerid, COLOR_SYNTAX, "Invalid slot, or the slot specified is locked.");
	}
	if(!CrateBox[boxid][cbWep][value-1])
	{
		return SCM(playerid, COLOR_SYNTAX, "The slot specified contains no weapon which you can take.");
	}

	GivePlayerWeapon(playerid, CrateBox[boxid][cbWep][value-1], CrateBox[boxid][cbWepAmount][value-1]);
	SM(playerid, COLOR_AQUA, "** You have taken a %s from slot %i of your crate package.", GetWeaponNameEx(CrateBox[boxid][cbWep][value-1]), value);

	CrateBox[boxid][cbWep][value-1] = 0;
	CrateBox[boxid][cbWepAmount][value-1] = 0;

	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE crates SET Gun%i = 0 WHERE id = %i", value, CrateBox[boxid][cbID]);
	mysql_tquery(connectionID, queryBuffer);
	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE crates SET GunAmount%i = 0 WHERE id = %i", value, CrateBox[boxid][cbID]);
	mysql_tquery(connectionID, queryBuffer);
	return 1;
}

