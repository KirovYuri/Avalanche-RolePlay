	new vehid = GetPlayerVehicleID(playerid), id = GetVehicleInfront(vehid), str[128];
	if(id < 0)
	{
		PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][0], "---"); // Speed
		PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][1], "_"); // Registration
		PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][2], "---~n~MODELID: ---"); // Model
		PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][3], "_"); // Registration (plate)
	}
	else
	{
		format(str, sizeof(str), "%.0f", GetVehicleSpeedMPH(id));
		PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][0], str); // Speed
		if(!strcmp(VehicleInfo[id][vPlate], "None", true)) {
			format(str, sizeof(str), "%02d", id);
			PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][1], str); // Registration
			PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][3], "_"); // Registration (plate)
		} else {
			format(str, sizeof(str), "%s", VehicleInfo[id][vPlate]);
			PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][1], "_"); // Registration
			PlayerTextDrawSetString(playerid, PlaceRadar_Player[playerid][3], str); // Registration (plate)
		}

		format(str, sizeof(str), "%s~n~MODELID: %02d", GetVehicleName(id), GetVehicleModel(id));
