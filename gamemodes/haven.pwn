	}
	return 1;
}

CMD:broadcast(playerid, params[])
{
	new style, text[128];

    if(PlayerInfo[playerid][pAdmin] < 3)
	{
	    return SCM(playerid, COLOR_SYNTAX, "You are not authorized to use this command.");
